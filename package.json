    "@types/uuid": "^9.0.8",
    "@types/leaflet": "^1.9.8",
    "babel-plugin-react-compiler": "latest",
    "eslint": "^8.57.0",
    "eslint-plugin-react-compiler": "latest",
    "eslint-plugin-react-hooks": "latest",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2"
  }
}