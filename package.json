{
  "name": "agentic-ai-chat",
  "version": "1.0.0",
  "description": "A responsive chat application powered by the Gemini API, featuring an agentic AI persona.",
  "keywords": [
    "react",
    "gemini",
    "ai",
    "chat",
    "agentic",
    "math"
  ],
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "tsc && vite build && npx tailwindcss -i ./src/styles/main.css -o ./dist/styles/main.css --minify",
    "build:server": "esbuild backend/server.ts --bundle --platform=node --format=cjs --outfile=dist/server.cjs --external:playwright",
    "start": "node dist/server.cjs",
    "dev": "node dev.js",
    "deploy": "npm run build:client && wrangler pages deploy dist"
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "^2.19.8",
    "@google/genai": "1.29.1",
    "@hookform/resolvers": "^3.3.4",
    "@million/lint": "^1.0.0-rc.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.28.0",
    "@vitejs/plugin-react": "^4.2.1",
    "ai": "^3.2.18",
    "autoprefixer": "^10.4.14",
    "echarts": "^5.5.0",
    "echarts-for-react": "^3.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "cpx": "^1.5.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "esbuild": "^0.20.2",
    "express": "^4.19.2",
    "framer-motion": "11.2.10",
    "html2canvas": "1.4.1",
    "idb-keyval": "^6.2.1",
    "jszip": "^3.10.1",
    "katex": "^0.16.9",
    "leaflet": "^1.9.4",
    "leaflet-control-geocoder": "^2.4.0",
    "react-leaflet": "^4.2.1",
    "lucide-react": "^0.363.0",
    "playwright": "^1.49.0",
    "postcss": "^8.4.30",
    "react": "18.3.1",
    "react-compiler-runtime": "latest",
    "react-dom": "18.3.1",
    "react-dropzone": "^14.2.3",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.51.1",
    "react-katex": "^3.0.4",
    "react-markdown": "^9.0.1",
    "react-resizable-panels": "^2.0.19",
    "react-syntax-highlighter": "15.5.0",
    "react-virtuoso": "^4.12.4",
    "rehype-katex": "^7.0.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "6.0.0",
    "sonner": "^1.4.41",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3.4.8",
